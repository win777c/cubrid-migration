            name="%migrationReport">
      </editor>
      <editor
            class="com.cubrid.cubridmigration.ui.history.CSVImportReportEditorPart"
            default="false"
            id="com.cubrid.cubridmigration.ui.history.CSVImportReportEditorPart"
            name="%migrationReport">
      </editor>
      <editor
            class="com.cubrid.cubridmigration.ui.history.CTCReportEditorPart"
