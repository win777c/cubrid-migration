      </editor>
   </extension>
    <extension
         point="org.eclipse.ui.editors">
      <editor
            class="com.cubrid.cubridmigration.ui.product.CMTNewInfoEditorPart"
            default="false"
            icon="icon/new_info.png"
            id="com.cubrid.cubridmigration.ui.product.CMTNewInfoEditorPart"
            name="%CMTNewEditor">
      </editor>
   </extension>
    <extension
         point="org.eclipse.ui.views">
      <category
            id="com.cubrid.cubridmigration.app.category1"
            name="CUBRID Migration Toolkit">
      </category>
      <view
            allowMultiple="false"
            category="com.cubrid.cubridmigration.app.category1"
            class="com.cubrid.cubridmigration.ui.script.MigrationScriptExplorerView"
            icon="icon/exportReport.gif"
            id="com.cubrid.cubridmigration.ui.script.MigrationScriptExplorerView"
            name="%migrationScriptExplorer"
            restorable="true">
      </view>
   </extension>
 <extension
       id="com.cubrid.migration.ui.commands.backup"
       name="Back Up Commands"
       point="org.eclipse.ui.commands">
    <command
          categoryId="com.cubrid.cubridmigration.ui.commands.category.window"
          id="com.cubrid.cubridmigration.ui.command.showDBExplore"
          name="%databaseExplorer">
    </command>
    <command
          id="com.cubrid.cubridmigration.ui.command.QuickStart"
          name="%quickStart">
    </command>
 </extension>
    
    <extension
          id="com.cubrid.migration.ui.commands.common"
          name="Common Commands"
          point="org.eclipse.ui.commands">
       <category
             id="com.cubrid.cubridmigration.app.commands.category.migration"
             name="Migration">
       </category>
    <category
          id="com.cubrid.cubridmigration.ui.commands.category.window"
          name="Window">
    </category>
    <category
          id="com.cubrid.cubridmigration.ui.commands.category.script"
          name="Migration Script">
    </category>
    <category
          id="com.cubrid.cubridmigration.ui.commands.category.help"
          name="Help">
    </category>
    <command
          categoryId="com.cubrid.cubridmigration.ui.commands.category.script"
          id="com.cubrid.cubridmigration.ui.command.NewMigration"
          name="%newMigration">
    </command>
    <command
          categoryId="com.cubrid.cubridmigration.app.commands.category.migration"
          id="com.cubrid.cubridmigration.ui.command.MigrationHistory"
          name="%migrationHistory">
    </command>
    <command
          categoryId="com.cubrid.cubridmigration.app.commands.category.migration"
          id="com.cubrid.cubridmigration.ui.command.Preference"
          name="%preference">
    </command>
    <command
          categoryId="com.cubrid.cubridmigration.ui.commands.category.window"
          id="com.cubrid.cubridmigration.app.commands.minimizeWindowCommand"
          name="%Minimize">
    </command>
    <command
          categoryId="com.cubrid.cubridmigration.ui.commands.category.window"
          id="com.cubrid.cubridmigration.app.command.zoomWindowCommand"
          name="%ZoomWindow">
    </command>
    <command
          categoryId="com.cubrid.cubridmigration.ui.commands.category.help"
          id="com.cubrid.cubridmigration.ui.command.OnlineHelp"
          name="%onlineHelp">
    </command>
    <command
          categoryId="com.cubrid.cubridmigration.ui.commands.category.help"
          id="com.cubrid.cubridmigration.ui.command.About"
          name="%about">
    </command>
    <command
          categoryId="com.cubrid.cubridmigration.ui.commands.category.script"
